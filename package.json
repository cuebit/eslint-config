{
  "name": "@cuebit/eslint-config",
  "type": "module",
  "version": "3.0.0",
  "packageManager": "pnpm@10.6.1",
  "description": "Opinionated ESLint config",
  "author": "Cue <hello@cuebit.dev>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/cuebit",
  "homepage": "https://github.com/cuebit/eslint-config",
  "keywords": [
    "eslint-config"
  ],
  "exports": {
    ".": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup --clean --dts",
    "dev": "tsup --watch & pnpm dlx @eslint/config-inspector",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && pnpm publish",
    "prepack": "pnpm build"
  },
  "peerDependencies": {
    "eslint": "catalog:"
  },
  "dependencies": {
    "@antfu/eslint-config": "catalog:",
    "defu": "catalog:"
  },
  "devDependencies": {
    "bumpp": "catalog:",
    "eslint": "catalog:",
    "tsup": "catalog:",
    "typescript": "catalog:"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
