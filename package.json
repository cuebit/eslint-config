{
  "name": "@cuebit/eslint-config",
  "type": "module",
  "version": "2.0.1-beta.3",
  "packageManager": "pnpm@9.13.0",
  "description": "Opinionated ESLint config.",
  "author": "Cue <hello@cuebit.digital>",
  "license": "MIT",
  "homepage": "https://github.com/cuebit/eslint-config",
  "keywords": [
    "eslint-config"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup --format esm,cjs --clean --dts",
    "stub": "tsup --format esm",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "release": "bumpp package.json && pnpm publish --access public",
    "prepublish": "pnpm run build",
    "prepare": "pnpm run build"
  },
  "dependencies": {
    "@antfu/eslint-config": "3.8.0",
    "defu": "^6.1.4",
    "eslint": "9.14.0",
    "eslint-flat-config-utils": "0.4.0",
    "eslint-plugin-format": "0.1.2"
  },
  "devDependencies": {
    "@cuebit/eslint-config": "link:.",
    "bumpp": "^9.8.1",
    "tsup": "^8.3.5",
    "typescript": "^5.6.3"
  }
}
