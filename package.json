{
  "name": "@cuebit/eslint-config",
  "type": "module",
  "version": "4.0.1",
  "packageManager": "pnpm@10.13.1",
  "description": "Opinionated ESLint config",
  "author": "Cue <hello@cuebit.dev>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/cuebit",
  "homepage": "https://github.com/cuebit/eslint-config",
  "keywords": ["eslint", "config"],
  "exports": {
    ".": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup --clean --dts",
    "dev": "tsup --watch & pnpm dlx @eslint/config-inspector",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && pnpm publish",
    "prepack": "pnpm build"
  },
  "peerDependencies": {
    "eslint": "catalog:"
  },
  "dependencies": {
    "@antfu/eslint-config": "^4.17.0",
    "defu": "^6.1.4",
    "eslint-flat-config-utils": "^2.1.0"
  },
  "devDependencies": {
    "bumpp": "^10.2.0",
    "eslint": "catalog:",
    "jiti": "^2.4.2",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  }
}
